{
  "name": "LlamaEnc3",
  "version": "3.4.0",
  "main": "./html/index.html",
  "description":
    "An ffmpeg wrapper for encoding videos for Vividcon and the Vidding community",
  "author": "Ian Roberts <ian@absolutedestiny.org>",
  "window": {
    "title": "LlamaEnc3",
    "toolbar": true,
    "frame": true,
    "show": false,
    "icon": "llama.png",
    "position": "center"
  },
  "platformOverrides": {
    "win": {
      "window": {
        "toolbar": false
      }
    },
    "osx": {
      "window": {
        "toolbar": false
      }
    },
    "linux": {
      "window": {
        "toolbar": false
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbsoluteDestiny/LlamaEnc3"
  },
  "bugs": {
    "url": "https://github.com/AbsoluteDestiny/LlamaEnc3/issues"
  },
  "dependencies": {
    "async": "0.9.0",
    "del": "^1.1.1",
    "file-extractor": "^0.6.1",
    "fluent-ffmpeg":
      "https://github.com/fluent-ffmpeg/node-fluent-ffmpeg/tarball/master",
    "github": "^0.2.3",
    "humanize-duration": "^3.7.0",
    "jQuery": "1.7.4",
    "lodash": "2.4.1",
    "nedb": "^1.1.1",
    "node-ffprobe": "git://github.com/nuky89/node-ffprobe",
    "open": "https://registry.npmjs.org/open/-/open-0.0.5.tgz",
    "sanitize-filename": "^1.1.1",
    "semver": "^4.2.0",
    "slice-file": "0.2.1",
    "temp": "^0.8.1"
  },
  "optionalDependencies": {
    "@ffmpeg-installer/darwin-x64": "^4.0.3",
    "@ffmpeg-installer/linux-ia32": "^4.0.2",
    "@ffmpeg-installer/linux-x64": "^4.0.2",
    "@ffmpeg-installer/win32-ia32": "^4.0.2",
    "@ffmpeg-installer/win32-x64": "^4.0.1",
    "@ffprobe-installer/darwin-x64": "4.0.2",
    "@ffprobe-installer/linux-ia32": "4.0.2",
    "@ffprobe-installer/linux-x64": "4.0.2",
    "@ffprobe-installer/win32-ia32": "4.0.1",
    "@ffprobe-installer/win32-x64": "4.0.1"
  },
  "preferGlobal": true,
  "private": true,
  "analyze": true,
  "license": "MIT",
  "devDependencies": {
    "archiver": "^1.0.0",
    "async": "^0.9.0",
    "glob-copy": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-archiver": "^1.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-filter": "^2.0.0",
    "gulp-flatten": "0.0.4",
    "gulp-jszip": "^0.1.2",
    "gulp-zip": "^2.0.3",
    "nw-builder": "^2.2.0"
  }
}
